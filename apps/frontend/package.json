{
  "name": "@msspbiz/frontend",
  "version": "0.1.0-alpha.1",
  "description": "MsspBizCenter Frontend Web App",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
